type Address {
  street: String
}

type Account {
  id: String!
  type: String!
  displayName: String
  name: String
  email: String
  phone: String
  website: String
  description: String
  addresses: [Address]
}

# The Root Query for the application
type Query {
    accounts(page: Int, size: Int, sort: String): [Account]!
}

schema {
  query: Query
}
