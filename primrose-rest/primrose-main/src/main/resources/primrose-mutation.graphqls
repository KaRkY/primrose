input InputAddress {
  type: String!
  street: String!
  streetNumber: String!
  city: String!
  postalCode: String!
  state: String
  country: String!
}

input InputContact {
  type: String!
  name: String!
  email: String
  phone: String
}

input InputAccount {
  type: String!
  displayName: String
  name: String
  email: String
  phone: String
  website: String
  description: String
  addresses: [InputAddress]
  contacts: [InputContact]
}
